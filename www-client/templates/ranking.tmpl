<div class="container-fluid">
  <div class="row-fluid">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>No</th>
          <th>Name</th>
          <th>Score</th>
	  {{#each problems}}
          <th>{{this}}</th>
	  {{/each}}
        </tr>
      </thead>
      <tbody>
        {{#each ranking}}
        <tr>
          <td>{{no}}</td>
          <td>{{name}}</td>
          <td>{{score}}</td>
	  {{#each problems}}
          <td>
            {{#if solved}}
            <span class="label label-success">{{points}}</span>
            {{else}}
            <span class="label">{{points}}</span>
	    {{/if}}
          </td>
          {{/each}}
        </tr>
        {{/each}}
     </tbody>
    </table>
  </div>
</div>
