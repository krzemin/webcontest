<section id="problem">
	<h1>{{problem.name}}</h1>
	<div class="pull-right">
		<dl class="dl-horizontal">
		  <dt>Time limit</dt>
		  <dd>{{problem.limits.time}}</dd>
		  <dt>Memory limit</dt>
		  <dd>{{problem.limits.memory}}</dd>
		</dl>
	</div>
	{{{problem.content}}}
	<section id="input">
	<h3>Input</h3>
	{{{problem.input}}}
	</section>
	<section id="output">
	<h3>Output</h3>
	{{{problem.output}}}
	</section>
	<section id="examples">
	<h3>Examples</h3>
	<div class="container">
		{{#each problem.examples}}
		<h4>{{name}}</h4>
		<div class="row">
		  <div class="span6">
		  	<h5>Sample input</h5>
		  	<pre class="pre-scrollable">{{input}}</pre>
		  </div>
		  <div class="span6">
		  	<h5>Sample output</h5>
		  	<pre class="pre-scrollable">{{output}}</pre>
		  </div>
		</div>
		{{#if explanation}}
		<p>{{explanation}}</p>
		{{/if}}
		{{/each}}
	</div>
	</section>
</section>
<section id="code">
	<h2>Code</h2>
	<div class="btn-toolbar">
	    <button class="btn"><i class="icon-hdd"></i> Save</button>
	    <button class="btn"><i class="icon-cog"></i> Compile</button>
	    {{!<button class="btn"><i class="icon-check"></i> Test</button>}}
	    <button class="btn btn-primary"><i class="icon-upload"></i> Submit</button>
	    <button class="btn pull-right" id="fullscreen"><i class="icon-fullscreen"></i> Fulscreen</button>
	</div>
	<section id="code-editor">
		<textarea id="codemirror">{{code.text}}</textarea>
	</section>
	<section id="code-messages">
		<a nohref id="toggle-messages">Show/Hide messages</a>
		<pre id="messages" class="pre-scrollable">Saving code...</pre>
	</section>
</section>
