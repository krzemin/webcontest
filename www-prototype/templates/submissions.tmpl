<h2>Submissions</h2>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Submission</th>
      <th>Status</th>
      <th>Score</th>
      <th class="hidden-phone" colspan="2">Performance</th>
    </tr>
  </thead>
  <tbody>
  	{{#each this}}
  	<tr data-id="{{id}}">
      <td><a nohref="">{{timestamp}}</a></td>
      <td>{{submission_status this}}</td>
      <td>{{score}}</td>
      <td class="hidden-phone">
      {{#if performance.time}}
    		<nobr><i class="icon-time"></i> {{performance.time}}</nobr> &nbsp;
    	{{/if}}
      </td>
      <td class="hidden-phone">
    	{{#if performance.memory}}
      	<nobr><i class="icon-tasks"></i> {{performance.memory}}</nobr>
      {{/if}}
      </td>
    </tr>
  	{{/each}}
  </tbody>
</table>