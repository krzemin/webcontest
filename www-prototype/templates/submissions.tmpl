<h2>Submissions</h2>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Submission</th>
      <th>Status</th>
      <th>Performance</th>
      <th>Score</th>
    </tr>
  </thead>
  <tbody>
  	{{#each this}}
  	<tr data-id="{{id}}">
      <td><a nohref="">{{timestamp}}</a></td>
      <td>{{submission_status this}}</td>
      <td>
      	{{#if performance.time}}
    		<i class="icon-time"></i> {{performance.time}} &nbsp;
    	{{/if}}
    	{{#if performance.memory}}
      		<i class="icon-tasks"></i> {{performance.memory}}
      	{{/if}}
      </td>
      <td>{{score}}</td>
    </tr>
  	{{/each}}
  </tbody>
</table>